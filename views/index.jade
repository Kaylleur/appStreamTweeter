extends layout

block content
  h1= title
  p Welcome to #{title}
  form.form-inline(method="POST" action="./")
      input.form-control(type="text" placeholder="search" name="search")
      input.btn.btn-primary(type="submit" value="Search")
  h2 Last tweets
  ul
  each val in data
    li #{val._source.created_at}:#{val._source.text}


